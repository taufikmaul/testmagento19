<?xml version="1.0"?>
<layout version="0.1.0">
    <default>
        <reference name="header">
            <block type="core/template" name="link.list.taufik" template="taufik_list/link.phtml"></block>
        </reference>
    </default>
    <list_index_index>
        <reference name="root">
            <action method="setTemplate"><template>page/1column.phtml</template></action>
        </reference>
        <reference name="content">
            <block type="list/listproduct" name="list_index" template="taufik_list/index.phtml" />
        </reference>
    </list_index_index>
    <list_index_taufik>
        <reference name="root">
            <action method="setTemplate"><template>page/1column.phtml</template></action>
        </reference>
        <reference name="content">
            <block type="list/listtaufik" name="list_index_taufik" template="taufik_list/taufik.phtml" />
        </reference>
    </list_index_taufik>
</layout>